import java.util.*;
public class Assignment2_part2 {
    public static void main(String[] args){
        int n;
        pattern13(n=6);
    }
    static void pattern13(int n){
        for ( int i = 0; i <n ; i++) {
            int icj=1;
            for (int j = 0; j <= i ; j++) {
                System.out.print(icj+ " ");
                int icjp1  = icj * (i-j) / (j+1);
                icj = icjp1;

            } System.out.println();
        }

    }
}
