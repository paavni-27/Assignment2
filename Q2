public class Assignment2 {
    public static void main(String[] args) {
        int n;
        pattern2(n = 5);
    }

    static void pattern2(int n){
        for (int i = 1; i <=n ; i++) {
            for (int j = 1; j <=n-i+1 ; j++) {
                System.out.print(" * ");
            }
            System.out.println();
        }
    }
  } 
