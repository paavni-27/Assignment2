public class Assignment2_part2 {
    public static void main(String[] args){
        int n;
        pattern14(n=6);
    }
    static void pattern14(int n){
        for ( int i = 1; i <= 10; i++) {
            int prod = n * i;
                System.out.println(n + " * " + i + " = " + prod);

            }
        }

    }
