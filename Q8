public class Assignment2 {
    public static void main(String[] args) {
        int n;
        pattern8(n = 5);
    }

     static void pattern8(int n){
         for(int i = 1; i <= n; i++){
             for (int k =1; k <= n-i ; k++) {
                 System.out.print(" ");
             }
             for (int j = 1; j <=1 ; j++) {
                 System.out.print("*");
             }
             System.out.println();
             }
         }
       }  
